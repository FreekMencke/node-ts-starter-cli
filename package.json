{
  "name": "node-ts-starter-cli",
  "version": "0.0.3",
  "description": "Generate new NodeJS projects with TypeScript and esbuild.",
  "author": "Freek Mencke",
  "license": "MIT",
  "homepage": "https://github.com/FreekMencke/node-ts-starter-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/FreekMencke/node-ts-starter-cli.git"
  },
  "bin": "./bin/node-ts-starter-cli",
  "scripts": {
    "cz": "git cz",
    "lint": "eslint . -c .eslintrc.js",
    "version:dev": "npm version prerelease --preid=dev",
    "publish:dev": "npm publish -tag=dev"
  },
  "dependencies": {
    "fs-extra": "^11.1.0",
    "lodash.merge": "^4.6.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.31.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "cli",
    "node",
    "nodejs",
    "ts",
    "typescript",
    "boilerplate",
    "generator",
    "starter",
    "template"
  ]
}
